{
    "name": "milan-miscevic/yii2crud",
    "description": "Basic CRUD operations in the Yii2 framework",
    "keywords": ["yii2", "crud", "basic"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "mmm\\yii2crud\\": "src"
        }
    },
    "require": {
        "yiisoft/yii2": "^2.0.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
